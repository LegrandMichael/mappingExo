{% extends 'base.html.twig' %}

{% block body %}
    <h1>Purchaseproduct</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ purchaseProduct.id }}</td>
            </tr>
            <tr>
                <th>Quantity</th>
                <td>{{ purchaseProduct.quantity }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('admin_purchaseproduct_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('admin_purchaseproduct_edit', { 'id': purchaseProduct.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
